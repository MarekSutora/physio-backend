{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  },
  "Cors": {
    "AllowedOrigin": "http://localhost:3000"
  },
  "ConnectionStrings": {
    "DefaultConnection": "x"
  },
  "JWTSettings": {
    "Key": "x",
    "Issuer": "x",
    "Audience": "x",
    "DurationInMinutes": 1440
  },
  "EmailSettings": {
    "DisplayName": "x",
    "SmtpUser": "x",
    "SmtpPass": "x",
    "SmtpHost": "x",
    "SmtpPort": 587
  }
}
