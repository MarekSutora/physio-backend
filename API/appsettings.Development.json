{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  },
  "Cors": {
    "AllowedOrigin": "https://localhost:3000"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=(LocalDb)\\LocalDB1;Database=Diploma-db;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "JWTSettings": {
    "Key": "MySuperSecretKeyMySuperSecretKeyMySuperSecretKey",
    "Issuer": "https://localhost:7048",
    "Audience": "https://localhost:7048",
    "DurationInMinutes": 10080 // 7 days  
  }
}
